{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "orderID": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "confirmed",
        "delivered",
        "cancelled"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "orders"
    },
    "totalAmount": {
      "type": "decimal"
    },
    "shippingAddress": {
      "type": "text"
    },
    "paymentMethod": {
      "type": "string"
    },
    "paymentStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "done"
      ]
    },
    "orderedAt": {
      "type": "datetime"
    },
    "shippedAt": {
      "type": "datetime"
    },
    "deliveredAt": {
      "type": "datetime"
    },
    "trackingId": {
      "type": "string"
    }
  }
}
