{
  "kind": "collectionType",
  "collectionName": "eye_powers",
  "info": {
    "singularName": "eye-power",
    "pluralName": "eye-powers",
    "displayName": "Eye Power",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 255,
      "private": false,
      "unique": false
    },
    "age": {
      "type": "integer",
      "min": 0,
      "max": 120,
      "private": false
    },
    "prescription_date": {
      "type": "date",
      "required": true
    },
    "clinic_name": {
      "type": "string",
      "maxLength": 255
    },
    "notes": {
      "type": "text",
      "private": true
    },
    "is_manualentry": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "right_eyeSPH": {
      "type": "decimal"
    },
    "right_eyeCYL": {
      "type": "decimal"
    },
    "right_eyeAXIS": {
      "type": "integer",
      "min": 0,
      "max": 180
    },
    "left_eyeSPH": {
      "type": "decimal"
    },
    "left_eyeCYL": {
      "type": "decimal"
    },
    "left_eyeAXIS": {
      "type": "integer",
      "min": 0,
      "max": 180
    },
    "prescription_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images", "files"]
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}