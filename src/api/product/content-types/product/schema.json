{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "integer"
    },
    "inStock": {
      "type": "boolean"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "orders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order.order",
      "mappedBy": "products"
    },
    "wishlistedByUsers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "color": {
      "type": "string"
    },
    "salesCount": {
      "type": "integer"
    },
    "lens_types": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lens-type.lens-type"
    },
    "lens_coatings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lens-coating.lens-coating"
    },
    "frame_weights": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::frame-weight.frame-weight"
    },
    "brands": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::brand.brand"
    },
    "frame_materials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::frame-material.frame-material"
    },
    "frame_shapes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::frame-shape.frame-shape"
    },
    "lens_thicknesses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lens-thickness.lens-thickness"
    },
    "frame_sizes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::frame-size.frame-size"
    }
  }
}
