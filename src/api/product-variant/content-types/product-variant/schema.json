{
  "kind": "collectionType",
  "collectionName": "product_variants",
  "info": {
    "singularName": "product-variant",
    "pluralName": "product-variants",
    "displayName": "Product Variant",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "variants"
    },
    "color_picker": {
      "type": "customField",
      "relation": "oneToOne",
      "target": "api::color.color",
      "customField": "plugin::color-picker.color"
    },
    "frame_size": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::frame-size.frame-size"
    },
    "inStock": {
      "type": "boolean",
      "required": true,
      "default": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "stock": {
      "type": "integer",
      "min": 1,
      "default": 0,
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "isActive": {
      "type": "boolean",
      "default": true,
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "salesCount": {
      "type": "integer",
      "min": 0,
      "default": 0,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}
