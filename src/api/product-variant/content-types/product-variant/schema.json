      "inversedBy": "variants"
    },
    "color": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::color.color"
    },
    "frame_size": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::frame-size.frame-size"
    },
    "inStock": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "stock": {
      "type": "integer",
      "min": 1,
      "default": 0,
      "required": true
    },
    "isActive": {
      "type": "boolean",
      "default": true,
      "required": true
    }
  }
}
