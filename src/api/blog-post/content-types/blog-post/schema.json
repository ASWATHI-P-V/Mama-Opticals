{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "BlogPost"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "content": {
      "type": "text"
    },
    "backgroundImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "authorName": {
      "type": "string"
    },
    "publishedDate": {
      "type": "datetime"
    },
    "eye_care_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::eye-care-category.eye-care-category"
    },
    "rating": {
      "type": "integer",
      "min": 1,
      "max": 5
    },
    "relatedBlogPosts": {
      "type": "json"
    }
  }
}
