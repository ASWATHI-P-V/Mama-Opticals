{
  "kind": "collectionType",
  "collectionName": "users-permissions_users",
  "info": {
    "name": "User",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "phone": {
      "type": "customField",
      "customField": "plugin::strapi-phone-validator.phone",
      "options": {
        "country": "in"
      },
      "unique": true,
      "configurable": false
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "unique": true,
      "required": true
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "profileImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "otp": {
      "type": "string",
      "private": true
    },
    "otpExpiryTime": {
      "type": "datetime",
      "private": true
    },
    "dateOfBirth": {
      "type": "date",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Other"
      ],
      "required": true
    },
    "tempNewPassword": {
      "type": "text",
      "private": true,
      "description": "Temporary storage for new password hash during password change flow"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "user"
    },
    "addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::address.address",
      "mappedBy": "user"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "user"
    },
    "customer_supports": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::customer-support.customer-support",
      "mappedBy": "user"
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "user"
    },
    "chat_sessions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chat-session.chat-session",
      "mappedBy": "user"
    },
    "ai_chat_messages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chat-message.chat-message",
      "mappedBy": "user"
    },
    "emailVerified": {
      "type": "boolean",
      "default": false
    },
    "phoneVerified": {
      "type": "boolean",
      "default": false
    },
    "emailVerificationCode": {
      "type": "string"
    },
    "phoneVerificationCode": {
      "type": "string"
    },
    "pendingEmail": {
      "type": "email",
      "private": true
    },
    "emailOtp": {
      "type": "string",
      "private": true
    },
    "emailOtpExpires": {
      "type": "datetime",
      "private": true
    },
    "pendingPhone": {
      "type": "string",
      "private": true
    },
    "phoneVerificationExpires": {
      "type": "datetime",
      "private": true
    }
  }
}
