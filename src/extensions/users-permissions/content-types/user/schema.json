{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "unique": true
    },
    "phone": {
      "type": "customField",
      "customField": "plugin::strapi-phone-validator.phone",
      "options": {
        "country": "in"
      },
      "unique": true
    },
    "otp": {
      "type": "integer"
    },
    "otpExpiryTime": {
      "type": "datetime"
    },
    "profileImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "user"
    }
  }
}
